prometheus:
  wal_directory: /tmp/grafana-agent-wal
  global:
    scrape_interval: 60s
  configs: null
integrations:
  node_exporter:
    enabled: true
  prometheus_remote_write:
    - url: https://prometheus-{{ grafana_location_prometheus }}.grafana.net/api/prom/push
      basic_auth:
        username: "{{ prometheus_user }}"
        password: {{ grafana_api_key }}
